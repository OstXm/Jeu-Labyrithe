////////// PIECE EN T ////////////////


for(int i = 0; i < 3; i++) {
        for (int j=0; j<3; j++){
            if (i==0 || (i==2 && j==0) || (i==2 && j==2)){
                printf("%c%c", 0xDB, 0xDB);
            }
            else {
                printf("  ");
            }
        }
        printf("\n");
    }
    
    
    
    
    
    
    
    
